: function mounted() {\n    this.onListar();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2GA;AACA;AACAA,oBADA;AAEAC,MAFA,kBAEA;AACA;AACAC,kBADA;AACA;AACAC,cAFA,CAEA;;AAFA;AAIA,GAPA;AAQAC;AACAC,cADA,sBACAC,OADA,EACA;AACA;AAAA;AAAA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AAAAA;AAAAC;AAAA;AACA;AACA,KARA;AASAC,iBATA,yBASAC,KATA,EASA;AACA;AACA,KAXA;AAYAC,YAZA,sBAYA;AACA;AACA;AACAC;AACA,KAhBA;AAkBAC,YAlBA,sBAkBA;AAAA;;AACA;AACA;AADA,QADA,CAIA;;AACA;AAEAC;AAAAC;AAAA,SACAC,IADA,CACA;AACAJ;AACA;AACA,OAJA,EAKAK,KALA,CAKA;AACAL;AACA,OAPA;AASA;AAlCA,GARA;AA4CAM,SA5CA,qBA4CA;AACA;AACA;AA9CA","names":["name","data","products","cart","methods","addProduct","product","quantity","removeProduct","index","sendCart","console","onListar","axios","headers","then","catch","mounted"],"sourceRoot":"src/components","sources":["HelloWorld.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"text-center\">\n      <v-col cols=\"6\">\n        <v-img\n          :src=\"require('../assets/logo-mw.png')\"\n          class=\"my-3\"\n          contain\n          height=\"200\"\n        />\n      </v-col>\n\n      <v-col class=\"mb-4\">\n        <h1 class=\"display-2 font-weight-bold mb-3\">\n          Teste Master da Web - Front\n        </h1>\n        \n\n    \n        \n\n      <div>\n      <v-card>\n        <v-card-title>Carrinho de Compras</v-card-title>\n        <v-card-text>\n          \n            <div v-for=\"(product, index) in products\" :key=\"index\">\n              <div class=\"d-flex justify-start align-center\">\n                <div>{{ product.name }} - R$ {{ product.preco }}</div>\n                <v-btn\n                  class=\"ml-auto\"\n                  fab\n                  dark\n                  x-small\n                  color=\"purple\"\n                  @click=\"addProduct(product)\"\n                >\n                  <v-icon dark>\n                    mdi-plus\n                  </v-icon>\n                </v-btn>\n              </div>\n            </div>\n          \n        </v-card-text>\n      </v-card>\n\n      <v-card>\n        <v-card-title>Itens no Carrinho</v-card-title>\n        <v-card-text>\n            <div v-for=\"(item, index) in cart\" :key=\"index\">\n              <div class=\"d-flex justify-start align-center\">\n                <div>\n                  {{ item.product.name }} - R$ {{ item.product.preco }} (Quantidade: {{ item.quantity }})\n                </div>\n                <v-btn\n                  class=\"ml-auto\"\n                  fab\n                  dark\n                  x-small\n                  color=\"error\"\n                  @click=\"removeProduct(index)\"\n                >\n                  <v-icon dark>\n                    mdi-delete\n                  </v-icon>\n                </v-btn>\n              </div>\n            </div>\n             <v-btn color=\"primary\" @click=\"sendCart\">Finalizar Compra</v-btn>\n        </v-card-text>\n      </v-card>\n       \n    </div>\n\n\n    \n\n\n    \n\n\n      <!-- <table class=\"table table-striped\">\n        <thead>\n          <tr>\n            <th>ID</th>\n            <th>Name</th>\n            <th>Preço</th>\n          </tr>\n        </thead>\n        <tbody>\n        <tr v-for=\"pro in products\" :key=\"pro.id\">\n          <td>{{ pro.id }}</td>\n          <td>{{ pro.name }}</td>\n          <td>  R$ {{ pro.preco  }}</td>\n        </tr>\n        </tbody>\n      </table> -->\n      \n      </v-col>      \n    </v-row>\n    <!-- <button button class=\"btn btn-primary\" @click=\"onListar()\">Consultar</button> -->\n  </v-container>\n</template>\n\n\n<script>\nimport axios from 'axios'\n  export default {\n    name: 'HelloWorld',\n    data () {\n    return {\n      products: [], // Array de produtos recebidos do JSON\n      cart: [] // Array de produtos adicionados ao carrinho\n    }\n  },\n  methods: {\n    addProduct(product) {\n      const itemIndex = this.cart.findIndex(item => item.product.id === product.id);\n      if (itemIndex !== -1) {\n        this.cart[itemIndex].quantity++;\n      } else {\n        this.cart.push({ product: product, quantity: 1 });\n      }\n    },\n    removeProduct(index) {\n      this.cart.splice(index, 1);\n    },\n    sendCart() {\n      // Aqui você pode enviar o carrinho (this.cart) de volta para o back-end\n      // utilizando uma requisição HTTP, por exemplo.\n      console.log(this.cart);\n    },\n\n    onListar () {\n      let headers = {\n        'Access-Control-Allow-Origin': '*'\n      }\n      // aqui colocar url da api que consulta os produtos\n      const baseURI = 'http://127.0.0.1:8000/api/produtos';\n\n      axios.get(baseURI, { headers: headers })\n        .then(response => {\n          console.log(response)\n          this.products = response.data\n        })\n        .catch(error => {\n          console.log(error)\n        })\n      \n    }\n  },\n  mounted() {\n    this.onListar();\n  }\n  }\n</script>\n"]},"metadata":{},"sourceType":"module"}